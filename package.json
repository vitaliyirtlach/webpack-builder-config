{
   "name": "webpack-builder-config",
   "version": "1.0.19",
   "description": "Webpack Config Builder",
   "main": "dist/index.js",
   "bin": {
      "build-config": "dist/index.js"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "pretty": "prettier src/",
      "lint": "eslint ",
      "build": "tsc",
      "prepublish": "npm run build",
      "test": "jest",
      "prepare": "husky install"
   },
   "keywords": [
      "webpack",
      "webpack-builder",
      "react",
      "svelte",
      "typescript"
   ],
   "bugs": {
      "url": "https://github.com/vitaliyirtlach/webpack-builder-config/issues"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/vitaliyirtlach/webpack-builder-config"
   },
   "author": "vitaliyirtlach <vitaliyirtlach@gmail.com>",
   "license": "ISC",
   "devDependencies": {
      "@types/chalk": "^2.2.0",
      "@types/fs-extra": "^9.0.8",
      "@types/inquirer": "^7.3.1",
      "@types/jest": "^26.0.20",
      "@types/node": "^14.14.33",
      "@types/shelljs": "^0.8.8",
      "chalk": "^4.1.0",
      "eslint": "^7.22.0",
      "fs-extra": "^9.1.0",
      "husky": "^6.0.0",
      "inquirer": "^8.0.0",
      "jest": "^26.6.3",
      "prettier": "2.2.1",
      "shelljs": "^0.8.4",
      "ts-jest": "^26.5.3",
      "ts-node": "^9.1.1",
      "ts-node-dev": "^1.1.6",
      "tslint": "^6.1.3",
      "typescript": "^4.2.3"
   },
   "dependencies": {
      "chalk": "^4.1.0",
      "fs-extra": "^9.1.0",
      "inquirer": "^8.0.0",
      "shelljs": "^0.8.4",
      "ts-node": "^9.1.1"
   },
   "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "transform": {
         "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
      },
      "transformIgnorePatterns": [
         "node_modules/(?!variables/.*)",
         "^.+\\.js$"
      ]
   }
}
